<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkLabel" id="label_child">
    <property name="margin-top">6</property>
    <property name="margin-bottom">6</property>
    <property name="xalign">0</property>
  </object>
  <object class="GtkSeparator" id="separator_child">
    <property name="valign">center</property>
  </object>
  <object class="GtkLabel" id="category_child">
    <property name="margin-top">6</property>
    <property name="xalign">0</property>
    <style>
      <class name="caption-heading"/>
      <class name="dim-label"/>
    </style>
  </object>
  <template class="NwtySidebarViewSwitcherItemRow" parent="AdwBin">
    <property name="child">
      <object class="GtkBox">
        <property name="spacing">6</property>
        <child>
          <object class="AdwBin" id="bin">
            <property name="hexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkImage" id="select_icon">
            <property name="icon-name">object-select-symbolic</property>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
