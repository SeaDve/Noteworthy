<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="NwtyContentView" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="AdwHeaderBar">
        <property name="show-start-title-buttons" bind-source="NwtyContentView" bind-property="compact" bind-flags="sync-create"/>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow">
        <property name="vexpand">True</property>
        <property name="child">
          <object class="GtkSourceView" id="view">
          </object>
        </property>
      </object>
    </child>
  </template>
</interface>
